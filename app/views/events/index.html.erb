<section class="container mt-5">
  <!--セクションタイトル-->
  <h2 class="section-ttl row mb-3">スケジュール管理</h2>


  <%= month_calendar events: @events do |date, events| %>
    <%= date.day %>
    <% events.each do |event| %>
      <div>
        <%= link_to event.title, event_path(event.id), class: "text-dark link-secondary" %>
      </div>
    <% end %>
  
  <% end %>

</section>